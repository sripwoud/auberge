system:
  configured_version: '0.11.1'
  timezone: Europe/Berlin
  card_enabled: true
  cal_enabled: true
  invite_from: {{ admin_user_email }}
  dav_auth_type: Digest
  admin_passwordhash: '{{ baikal_admin_password_hash }}'
  auth_realm: BaikalDAV
  base_uri: ''

database:
  sqlite:
    file: Specific/db/db.sqlite
